    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "cwd": "${cwd}"
        },
        {
            "name": "pwsh   -scheme 'scheme test 1.jsonc'",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/psDrivePartititor.ps1",
            "args": ["-scheme 'scheme test 1.jsonc'"]
        },
        {
            "name": "pwsh   -scheme 'scheme test 1.jsonc' -debugMath",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/psDrivePartititor.ps1",
            "args": ["-scheme 'scheme test 1.jsonc'", "-debugMath"]
        },
        {
            "name": "pwsh   -scheme 'scheme test 2 clear drive.jsonc'",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/psDrivePartititor.ps1",
            "args": ["-scheme 'scheme test 2 clear drive.jsonc'"]
        },
        {}
    ]
}
